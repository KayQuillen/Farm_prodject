

<p id="notice"><%= notice %></p>


<h1>Cattle</h1>
<body>
<table class="table">
  <thead>
    <tr>
      <th>Tag</th>
      <th>Color</th>
      <th>Gender</th>
      <th>Heffer</th>
      <th>DOB</th>
      <th>Descendant of</th>
      <th>immunization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cattles.each do |cattle| %>
      <tr>
        <td><%= cattle.tag %></td>
        <td><%= cattle.color %></td>
        <td><%= cattle.gender %></td>
        <td><%= cattle.heffer %></td>
        <td><%= cattle.dob %></td>
        <td><%= cattle.descendant_of %></td>
        <td><%= cattle.immunization %></td>
        <td><%= link_to 'Show', cattle %></td>
        <td><%= link_to 'Edit', edit_cattle_path(cattle) %></td>
        <td><%= link_to 'Destroy', cattle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cattle', new_cattle_path %>
<br>
<%= link_to 'Home', pages_path %>
</body>
